					REPLACE(SUBSTRING(tu.Url,2,LEN(tu.Url)-1),'//','/') AS TabPath 
					FROM {databaseOwner}[{objectQualifier}vw_Modules] m 
					INNER JOIN {databaseOwner}[{objectQualifier}Tabs] t ON t.TabID = m.TabID  
